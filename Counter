    "/reports/dr": {
      "get": {
        "tags": [
          "SUSHIApi"
        ],
        "summary": "",
        "description": "This resource returns COUNTER \"Database Master Report\" [DR]. A customizable report detailing activity by database that allows the user to apply filters and select other configuration options for the report.",
        "parameters": [
          {
            "name": "customer_id",
            "in": "query",
            "description": "Identifies the customer to provide usage for.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "begin_date",
            "in": "query",
            "description": "Usage start date in the form of yyyy-mm-dd or yyyy-mm.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "end_date",
            "in": "query",
            "description": "Usage end date in the form of yyyy-mm-dd or yyyy-mm.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "platform",
            "in": "query",
            "description": "Name of the Platform the usage is being requested for.  This can be omitted if the service provides usage for only one platform.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "database",
            "in": "query",
            "description": "Name of a specific database usage is being requested for. If omitted, all databases on the platform with usage for the requested customer will be returned",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "metric_type",
            "in": "query",
            "description": "A report filter. A list of Metric_Types to return. Specify multiple values by delimiting with a | character.  Omitting this parameter will return all metric_types with statistics.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "data_type",
            "in": "query",
            "description": "A Report Filter. A list of Data_Types to return usage for.  Use | character to delimit multiple values. Omit this parameter to include all Data_Types with statistics.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "access_method",
            "in": "query",
            "description": "A Report Filter. A list of Access_Methods to return delimited with a | character.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "attributes_to_show",
            "in": "query",
            "description": "A Report Attribute. A list of attributes to include as separate elements in response (usage will be broken out by attribute values).  Multiple attribute names are delimited with a | character. Options include Data_Type and Access_Method. When fields are included usage will be presented for each value of that field (with usage) otherwise usage will be rolled up for all values of a given field.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "granularity",
            "in": "query",
            "description": "Optional Report Attribute. Include this parameter to allow usage to be retrieved with a granularity of \"month\" (default if omitted) or \"totals\"",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Database Master Report [DR].",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/COUNTERDatabaseReport"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/COUNTERDatabaseReport"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/COUNTERDatabaseReport"
                }
              }
            }
          },
          "400": {
            "description": "Error occurred processing request.",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/SUSHIErrorModel"
